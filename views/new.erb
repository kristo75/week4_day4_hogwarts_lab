<form class="" action="/students" method="POST">

<label for="first-name-input">First name:</label>
<input id="first-name-input" type="text" name="first_name" value="">

<label for="last-name-input">Last name:</label>
<input id="last-name-input" type="text" name="last_name" value="">

<label for="house-name-input">House name:</label>
<select class="" name="house_name" id="house-select">
  <% @houses.each do |house|  %>
  <option value=<% house.id  %>><% house.name.capitalize %> </option>
<% end %>
</select>

<label for="age-input">Age:</label>
<input id="age-input" type="text" name="age" value="">

<input type="submit" name="" value="Register Student">

</form>



